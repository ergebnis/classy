{
  "name": "localheinz/classy",
  "type": "library",
  "description": "Provides a way to collect classy constructs from source or a directory.",
  "homepage": "https://github.com/localheinz/classy",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "require": {
    "php": "^7.1"
  },
  "require-dev": {
    "infection/infection": "~0.11.2",
    "localheinz/php-cs-fixer-config": "~1.16.1",
    "localheinz/test-util": "0.2.2",
    "phpbench/phpbench": "~0.14.0",
    "phpstan/phpstan": "~0.10.5",
    "phpunit/phpunit": "^7.4.4"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Localheinz\\Classy\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Localheinz\\Classy\\Test\\": "test/"
    }
  },
  "support": {
    "issues": "https://github.com/localheinz/classy/issues",
    "source": "https://github.com/localheinz/classy"
  }
}
